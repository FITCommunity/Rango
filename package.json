{
  "name": "rango",
  "version": "1.0.0",
  "description": "Discord Bot for FIT | Community",
  "engines": {
    "node": "16.x"
  },
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "scripts": {
    "start": "node src/index.js",
    "prestart": "node src/deploy-commands.js",
    "test": "node src/index.js",
    "postinstall": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FITCommunity/Rango.git"
  },
  "author": "Enis MuliÄ‡",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FITCommunity/Rango/issues"
  },
  "homepage": "https://github.com/FITCommunity/Rango#readme",
  "dependencies": {
    "@discordjs/builders": "^0.5.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "discord-api-types": "^0.22.0",
    "discord.js": "^13.1.0",
    "dotenv": "^10.0.0",
    "imapflow": "^1.0.67",
    "node-cron": "^2.0.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.4",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2"
  }
}
